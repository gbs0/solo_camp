<div class="container">
  <%= simple_form_for [@analise, @analise_amostra] do |f|%>
    <%= f.input :amostra_id, collection: @analises, input_html: { multiple: true, class: "multiple-select mb-2" }, include_hidden: false %>
    <%= f.submit "Adicionar Amostra", class: "btn btn-primary"%>
  <% end %>
</div>

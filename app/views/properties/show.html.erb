<div class="mb-3 ml-5 mt-2"><h2><%= @property.name %></h2></div>
<div class="d-flex flex-row">
    <div class="ml-5 mt-3">
        <%= render partial: 'weather', locals: {celsius: @celsius, threshold_timestamp: @threshold_timestamp, timestamp: @timestamp, property: @property }%>
    </div>

    <div class="map mt-3">
        <img src="<%= @map %>">
    </div>
</div>
